apiVersion: apps/v1
kind: Deployment
metadata: 
  name: {{ deployment_name }}
  labels:
    app: nginx
spec:
  replicas: {{ replicas }}
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
        - name: nginx
          image: {{ deployment_name }}
          ports:
            - containerPort: 80
